@model QuizH.ViewModels.Question.QuestionDetailsViewModel
<div>
    <h4>Domanda</h4>
    <p>@Model.Text</p>
    <h4>Risposte</h4>
    @foreach(var option in Model.Options)
    {
        <p>@option</p>
    }
    <h4>Argomento</h4>
    <p>@Model.Subject</p>
    <h4>Corsi</h4>
    @foreach (var course in Model.Courses)
    {
        <p>@course</p>
    }
</div>