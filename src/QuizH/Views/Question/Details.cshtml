@model QuizH.ViewModels.Exam.ExamDetailsViewModel
<div class="row">
    <div class="col-md-9"><h1>@Model.Title</h1></div>
</div>
<div class="row">
    <div class="col-md-3"><h3>@Model.Course</h3></div>
</div>

<div class="row">
    <div class="col-md-12"><h2>Instructions</h2></div>
    <div class="col-md-12">@Model.Instructions</div>
</div>
<div class="row">
    <div class="col-md-12"><h2>Questions</h2></div>
    @foreach (var question in Model.Questions)
    {
        <div class="col-md-12">@question</div>
    }
</div>
