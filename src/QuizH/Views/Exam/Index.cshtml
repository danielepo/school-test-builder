@model QuizH.ViewModels.Exam.ExamListViewModel


<table class="table table-striped">
    <thead><tr><td>Title</td><td>Course</td></tr></thead>
    <tbody>

        @foreach (var exam in Model.Exams)
        {
            <tr>
                <td>@Html.Raw(exam.Title)</td>
                <td>@exam.Course</td>
                <td>@Html.ActionLink("Edit", "Edit", new { id = exam.Id })</td>
                <td>@Html.ActionLink("Details", "Details", new { id = exam.Id })</td>
            </tr>
        }
    </tbody>
</table>


@Html.ActionLink("Insert a new Exam", "Insert")
